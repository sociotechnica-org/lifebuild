{
  "name": "@lifebuild/shared",
  "version": "0.1.20",
  "type": "module",
  "license": "FSL-1.1-ALv2",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint-all": "pnpm lint:fix && pnpm format && pnpm typecheck"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./events": {
      "import": "./src/livestore/events.ts",
      "require": "./dist/livestore/events.js",
      "default": "./dist/livestore/events.js"
    },
    "./schema": {
      "import": "./src/livestore/schema.ts",
      "require": "./dist/livestore/schema.js",
      "default": "./dist/livestore/schema.js"
    },
    "./queries": {
      "import": "./src/livestore/queries.ts",
      "require": "./dist/livestore/queries.js",
      "default": "./dist/livestore/queries.js"
    },
    "./types": {
      "import": "./src/types.ts",
      "require": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./auth": {
      "import": "./src/auth/index.ts",
      "require": "./dist/auth/index.js",
      "default": "./dist/auth/index.js"
    },
    "./llm-tools": {
      "import": "./src/llm-tools/index.ts",
      "require": "./dist/llm-tools/index.js",
      "default": "./dist/llm-tools/index.js"
    },
    "./llm-tools/schemas": {
      "import": "./src/llm-tools/schemas.ts",
      "require": "./dist/llm-tools/schemas.js",
      "default": "./dist/llm-tools/schemas.js"
    },
    "./llm-tools/documents": {
      "import": "./src/llm-tools/documents.ts",
      "require": "./dist/llm-tools/documents.js",
      "default": "./dist/llm-tools/documents.js"
    },
    "./llm-tools/tasks": {
      "import": "./src/llm-tools/tasks.ts",
      "require": "./dist/llm-tools/tasks.js",
      "default": "./dist/llm-tools/tasks.js"
    },
    "./llm-tools/projects": {
      "import": "./src/llm-tools/projects.ts",
      "require": "./dist/llm-tools/projects.js",
      "default": "./dist/llm-tools/projects.js"
    },
    "./rooms": {
      "import": "./src/rooms.ts",
      "require": "./dist/rooms.js",
      "default": "./dist/rooms.js"
    },
    "./table-state": {
      "import": "./src/table-state.ts",
      "require": "./dist/table-state.js",
      "default": "./dist/table-state.js"
    },
    "./llm-tools/base": {
      "import": "./src/llm-tools/base.ts",
      "require": "./dist/llm-tools/base.js",
      "default": "./dist/llm-tools/base.js"
    },
    "./llm/models": {
      "import": "./src/llm/models.ts",
      "require": "./dist/llm/models.js",
      "default": "./dist/llm/models.js"
    }
  },
  "dependencies": {
    "@livestore/livestore": "catalog:",
    "@livestore/utils": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
