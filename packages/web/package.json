{
  "name": "@lifebuild/web",
  "version": "0.1.51",
  "type": "module",
  "private": true,
  "license": "FSL-1.1-ALv2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "storybook": "storybook dev -p 6010 --no-open",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook --url http://127.0.0.1:6008",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && npx http-server storybook-static --port 6008 --silent\" \"wait-on tcp:6008 && pnpm test:storybook\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint-all": "pnpm lint:fix && pnpm format && pnpm typecheck",
    "deploy": "wrangler pages deploy dist --project-name lifebuild-web"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
