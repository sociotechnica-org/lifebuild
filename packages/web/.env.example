# The URL for the LiveStore WebSocket server.
# For local development, this points to the local wrangler instance
VITE_LIVESTORE_SYNC_URL=ws://localhost:8787

# Store ID for local testing - must match server config
VITE_STORE_ID=test-store-fixed-materializers-001

# Auth token for local testing - must match server config
VITE_AUTH_TOKEN=insecure-token-change-me

# Authentication settings
VITE_REQUIRE_AUTH=false
VITE_AUTH_SERVICE_URL=http://localhost:8788

# Maximum iterations for agentic loop (default: 30)
VITE_LLM_MAX_ITERATIONS=30

# Maximum execution time for agentic loop in milliseconds (default: 600000 = 10 minutes)
VITE_LLM_MAX_EXECUTION_TIME_MS=600000

# Development mode
NODE_ENV=development