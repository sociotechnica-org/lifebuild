# The URL for the LiveStore WebSocket server.
# For local development, this should point to the local wrangler instance (e.g., ws://localhost:8787).
VITE_LIVESTORE_SYNC_URL=

# The ID of the Cloudflare D1 database.
# This is obtained after creating the database via the Wrangler CLI.
D1_DATABASE_ID= 

# API Key for Braintrust, LLM provider
BRAINTRUST_API_KEY=

# Maximum iterations for agentic loop (default: 15)
# Increase for complex multi-step operations, decrease to prevent runaway loops
VITE_LLM_MAX_ITERATIONS=15