# The URL for the LiveStore WebSocket server.
# For local development, this points to the local wrangler instance
VITE_LIVESTORE_SYNC_URL=ws://localhost:8787

# Store ID for local testing - must match server config
VITE_STORE_ID=test-store-fixed-materializers-001

# Auth token for local testing - must match server config
VITE_AUTH_TOKEN=insecure-token-change-me

# Authentication settings
VITE_REQUIRE_AUTH=false
VITE_AUTH_SERVICE_URL=http://localhost:8788

# Maximum iterations for agentic loop (default: 15)
VITE_LLM_MAX_ITERATIONS=15

# PostHog Analytics (via first-party proxy)
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=https://coconut.worksquared.ai

# Sentry Error Tracking
# Get your DSN from https://sentry.io/
# The same DSN can be used across environments - Sentry will separate events by MODE (development/production)
VITE_SENTRY_DSN=

# Sentry Source Maps Upload (for production builds only)
# Get these from https://sentry.io/settings/account/api/auth-tokens/
# Only needed when building for production with source maps
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# Development mode
NODE_ENV=development